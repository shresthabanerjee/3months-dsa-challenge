#include <iostream>
using namespace std;

int main() {
	int t;int n;int arr[100000];
	std::cin >> t;
	while(t--)
	{int sum=0;
	   	std::cin >> n;
	for(int i=0;i<n;i++)
	{
	    std::cin >> arr[i];
	    if(arr[i]==-1)
	    {
	        arr[i]=sum/i;
	        sum+=arr[i];
	    }
	    else
	    {
	        sum+=arr[i];
	    }
	}
	
	    for(int i=0;i<n;i++)
	{
	    std::cout << arr[i]<<" ";
	  
	}
	 std::cout << "\n" << std::endl;
}
	
	
	return 0;
}
